{
  "name": "datahub-local-react",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "server": "node server/index.js",
    "client:dev": "npm --prefix client run dev",
    "dev": "concurrently -n server,client \"npm run server\" \"npm run client:dev\"",
    "client:build": "npm --prefix client run build",
    "build": "npm run client:build",
    "start": "node server/index.js"
  },
  "dependencies": {
    "csv-parse": "^5.6.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  }
}
